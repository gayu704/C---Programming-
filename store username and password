#include <stdio.h>
#include <string.h>

int main() {
    char username[20], password[20];
    char u[20] = "admin";
    char p[20] = "1234";

    printf("Enter Username: ");
    scanf("%s", username);

    printf("Enter Password: ");
    scanf("%s", password);

    if(strcmp(username, u) == 0 && strcmp(password, p) == 0) {
        printf("Login Successful");
    } else {
        printf("Invalid Username or Password");
    }

    return 0;
}
