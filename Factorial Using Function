#include <stdio.h>

long long factorial(int num) {
    int i;
    long long fact = 1;

    for (i = 1; i <= num; i++) {
        fact = fact * i;
    }
    return fact;
}

int main() {
    int num;
    long long result;

    printf("Enter a number: ");
    scanf("%d", &num);

    result = factorial(num);

    printf("Factorial = %lld", result);

    return 0;
}
