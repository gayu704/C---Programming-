#include <stdio.h>

int checkPrime(int num) {
    int i;
    if (num <= 1)
        return 0;

    for (i = 2; i <= num / 2; i++) {
        if (num % i == 0)
            return 0;
    }
    return 1;
}

int main() {
    int num;

    printf("Enter a number: ");
    scanf("%d", &num);

    if (checkPrime(num))
        printf("Prime Number");
    else
        printf("Not a Prime Number");

    return 0;
}
